#algolia-shopify-instantsearch .ais-facets{
    background: #fff;
    width: 100%;
    float: none;
}

#algolia-shopify-instantsearch .ais-block{
    background: #fff;
    width: 100%;
    float: none;
}

.ais-page .ais-Hits-list{
    display: flex;
    flex-wrap: wrap;
    padding-left: 0;
}

.ais-page .ais-Hits-item{
    padding: 10px;
    width: calc( 100% / 3 );
}

#algolia-shopify-instantsearch .ais-page .ais-page--body .ais-block .ais-hit-gh{
    cursor: pointer;
    border: 2px solid #eee;
    border-radius: 5px;
    padding: 20px 10px;
    height: 100%;
    width: 100%;
    float: none;
    margin: 0;
    text-align: center;
    display: flex;
    flex-direction: column;
    transition: .3s ease-in-out border-color, .3s ease-in-out box-shadow, background-position ease-in-out .2s;
}

#algolia-shopify-instantsearch .ais-page .ais-page--body .ais-block .ais-hit-gh:hover{
    border-color: var(--gh-color-green-dark);
    box-shadow: var(--gh-color-green-light) 0 0 12px;
}

.ais-page .ais-hit-gh--picture{
    text-align: center;
    padding-bottom: 20px;
}

.ais-page .ais-hit-gh--picture{

}

.ais-page .ais-hit-gh--picture img{
    width: 150px;
    transition: transform ease-in-out .25s;
}

.ais-page .ais-hit-gh:hover .ais-hit-gh--picture img{
    transform: scale(1.1);
}

.ais-page .ais-hit-gh--title{

}

.ais-page .ais-hit-gh--title a,
.ais-page a, .ais-page .ais-hit-gh--title a:hover{
    color: var(--gh-color-green-dark);
    font-weight: 700;
    font-size: 17px;
    text-decoration: none;
}

.ais-page a, .ais-page .ais-hit-gh--title a:hover{

}

.ais-page .ais-Hits-list span{
    font-size: inherit;
}

.ais-page .ais-hit-gh--title{
    line-height: 1.4;
}

.ais-page .ais-hit-gh--subtitle{
    line-height: 1.5;
}

.ais-page .ais-hit-gh--price{

}

.ais-hit-gh--details{
    flex: 1;
